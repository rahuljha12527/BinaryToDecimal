import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
		// Write your code here

        Scanner sc=new Scanner(System.in);
        
        int n=sc.nextInt();
        
        
        int rev=0;
        int base=1;
        while(n!=0){
            
            int rem=n%10;
            
            n/=10;
            rev=rev+rem*base;
                
             base*=2;   
            
            
        }
        
       System.out.println(rev);
	}
}
